{
	"name": "tests",
	"tree": {
		"$className": "DataModel",

		"HttpService": {
			"$className": "HttpService",

			"$properties": {
				"HttpEnabled": true
			}
		},

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",

			"Packages": {
				"$className": "Folder",
				"$path": "Packages",
				"$ignoreUnknownInstances": true,

				"package-template": { "$path": "src" }
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",

			"Tests": {
				"$className": "Folder",

				"Tests": {
					"$className": "Folder",
					"$path": "tests",
					"$ignoreUnknownInstances": true
				}
			}
		}
	}
}
